    #
    # response[counter]{rawtime,time,responsetime,client,direction}
    # rate[sec]=09:55:19
    # rate[min]=09:55
    # rate[hours]=09
    # rate[time][msgID]



            message=normalize()           

           if ($4 ~ /Message/){
                msgName=$5	
           }else{
                msgName=$4	
           }
        
