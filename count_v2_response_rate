}
function walk_array(arr, name, i){
    
    if (isarray(arr)){
        for (i in arr) {
        #print i "\n"
            if (isarray(arr[i]))
                walk_array(arr[i], (name "," i ","))
            else
                printf("%s,%s,%s\n", name, i, arr[i])
                
        }
    }else{
    print arr " is not array"
    }

}

